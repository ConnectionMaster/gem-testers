- content_for :javascripts do
  = javascript_include_tag 'jquery.dataTables'
  = javascript_include_tag 'test_results'
- content_for :stylesheets do
  = stylesheet_link_tag 'demo_table_jui'
- test_results = test_results.is_a?(Array) ? test_results : [test_results]
.test-results-table-wrapper.margins
  %table.test-results-table
    = render :partial => 'shared/test_result_headers'
    = render :partial => 'shared/test_result', :collection => test_results
  = yield
